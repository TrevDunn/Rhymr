<div class="main-section">
	<div class="main-heading">
		<h1> Log In </h1>
	</div>
	<%= form_tag '/login' do %>
		<div class="login">
		<ul class="login">
			<li>
				Email: <%= text_field_tag :email%>
			</li>
			<li>
				Password: <%= password_field_tag :password  %>
			</li>
		</ul>
		<div class="submit-button">
			<%= submit_tag "Submit" %>
		</div>
	</div>
	<% end %>
</div>
